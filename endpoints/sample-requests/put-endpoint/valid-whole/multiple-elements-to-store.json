{
  "description": "Put request wants to store multiple elements but no more than the maximum allowed by the 'max_num_values' config. Store them under a random UUIDs",
  "serverConfig": {
    "max_num_values": 2
  },
  "putRequest": {
    "puts": [
      {
        "type": "xml",
        "ttlseconds": 60,
        "value": "<?xml version=\"1.0\">__video_info__<\\/VAST>\r\n"
      },
      {
        "type": "json",
        "ttlseconds": 60,
        "value": "{\"an_int_field\": 1}"
      }
    ]
  },
  "expectedMetrics": {
    "putTotal": 1,
    "totalXmlRequests":1,
    "putBackendSize":5.00,
    "putBackendTTLSeconds":60.00,
    "putDuration": 1.00
  },
  "expectedResponse": {
    "responses": [
      {"uuid": "random"},
      {"uuid": "random"}
    ]
  }
}
