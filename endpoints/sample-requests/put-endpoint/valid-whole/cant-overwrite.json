{
  "description": "Prebid Cache doesn't allow stored entries to be rewritten. Don't return error, simply a regular response with a missing 'uuid' field",
  "serverConfig": {
    "allow_setting_keys": true,
    "stored_data_on_backend": [
      {
        "key":"the-custom-thrity-six-character-uuid",
        "value":"<tag>original_XML</tag>",
        "type":"xml",
        "ttlseconds":60
      }
    ]
  },
  "putRequest": {
    "puts":[
      {
        "type":"xml",
        "ttlseconds":60,
        "value":"<tag>updated_XML</tag>",
        "key":"the-custom-thrity-six-character-uuid"
      }
    ]
  },
  "expectedMetrics": [
    "RecordPutBackendTTLSeconds",
    "RecordPutDuration",
    "RecordPutBackendSize",
    "RecordPutBackendXml",
    "RecordPutKeyProvided",
    "RecordPutTotal",
    "RecordPutBackendError"
  ],
  "expectedResponse": {
    "responses": [
      {"uuid": ""}
    ]
  }
}


